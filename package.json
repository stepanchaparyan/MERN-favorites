{
  "name": "mern-favorites",
  "version": "1.0.0",
  "description": "Mongo, Express, React, Node",
  "main": "index.js",
  "scripts": {
    "server": "nodemon app.js",
    "dev-server": "cross-env NODE_ENV=development webpack-dev-server --mode development --open",
    "start": "cross-env NODE_ENV=development concurrently \"npm run server\" \"npm run dev-server\"",
    "build": "rm -rf build && cross-env NODE_ENV=production webpack --mode production",
    "dev": "rm -rf build && cross-env NODE_ENV=development webpack --mode development",
    "watch": "rm -rf build && cross-env NODE_ENV=development webpack --mode development --watch",
    "stats": "webpack --json > stats.json && webpack-bundle-analyzer stats.json",
    "eslint": "eslint .",
    "eslint-fix": "eslint --fix \"src/**/*.js\"",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|md|vue)\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stepanchaparyan/MERN-favorites.git"
  },
  "keywords": [
    "mongo",
    "express",
    "react",
    "node"
  ],
  "author": "Stepan Chaparyan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stepanchaparyan/MERN-favorites/issues"
  },
  "homepage": "https://github.com/stepanchaparyan/MERN-favorites#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run eslint",
      "git add"
    ],
    "*.{json|css|scss|md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.4",
    "normalize.css": "^8.0.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-router-dom": "^5.1.2",
    "react-select": "^3.1.0",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-transform-regenerator": "^7.8.7",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.2.0",
    "csv-loader": "^3.0.2",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.5.1",
    "express": "^4.17.1",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.12.0",
    "nodemon": "^2.0.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "papaparse": "^5.1.1",
    "prettier": "1.18.2",
    "sass": "^1.22.12",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.1.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.40.2",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "xml-loader": "^1.2.1"
  }
}
